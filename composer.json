{
    "name": "lavrahq/flarum-auth0",
    "description": "Allow users to log in with Auth0.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Frojd",
            "email": "martin@helmgast.se"
        },
        {
          "name": "Scott Plunkett",
          "email": "plunkets@aeoss.io"
        }
    ],
    "support": {
        "issues": "https://github.com/lavrahq/flarum-auth0/issues",
        "source": "https://github.com/lavrahq/flarum-auth0"
    },
    "require": {
        "riskio/oauth2-auth0": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Lavra\\Flarum\\Auth\\Auth0\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Auth0 Login",
            "icon": {
                "name": "fab fa-sign-in-alt",
                "backgroundColor": "#a00",
                "color": "#fff"
            }
        }
    }
}
